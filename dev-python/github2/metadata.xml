<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE pkgmetadata SYSTEM "http://www.gentoo.org/dtd/metadata.dtd">
<pkgmetadata>
	<herd>no-herd</herd>
	<maintainer>
		<email>jnrowe@gmail.com</email>
		<name>James Rowe</name>
	</maintainer>
	<longdescription lang="en">
		This is an experimental python library implementing all of the features
		available in version 2 of the Github API.
	</longdescription>
	<use>
		<flag name="proxy" restrict=">=dev-python/github2-0.4.0">
			Adds support for using a HTTP proxy with <pkg>dev-python/github2</pkg>
		</flag>
	</use>
	<upstream>
		<doc
			lang="en">https://github.com/ask/python-github2/blob/master/README.rst#readme</doc>
		<remote-id type="pypi">github2</remote-id>
		<bugs-to>https://github.com/ask/python-github2/issues</bugs-to>
	</upstream>
</pkgmetadata>
